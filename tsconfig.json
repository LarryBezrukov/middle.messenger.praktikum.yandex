{
	"compilerOptions": {
		"outDir": "dist",
		"allowSyntheticDefaultImports": true,
		"noImplicitAny": true,
		"module": "commonjs",
		"target": "es6",
		"allowJs": true,
		"moduleResolution": "node",
		"typeRoots": ["node_modules/@types", "typings/app.d.ts"],
		"skipLibCheck": true,
		"esModuleInterop": true
	}
}
